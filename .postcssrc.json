{
  "plugins": {
    "postcss-import": {},
    "tailwindcss/nesting": {},
    "tailwindcss": {},
    "autoprefixer": {},
    "postcss-px-to-viewport-8-plugin":{
      "unitToConvert": "px",
      "viewportWidth": 1920,
      "unitPrecision": 5,
      "propList": ["*"],
      "viewportUnit": "vw",
      "fontViewportUnit": "vw",
      "selectorBlackList": [],
      "minPixelValue": 1,
      "mediaQuery": false,
      "replace": true,
      "exclude": [],
      "landscape": false,
      "landscapeUnit": "vw",
      "landscapeWidth": 568
    }
  }
}